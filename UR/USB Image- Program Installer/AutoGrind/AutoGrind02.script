    local time_ms = MATH_PI * circle_diam_mm * n_cycles / grind_linear_vel_mps + 1500
    socket_send_integer("robot_step_time_estimate_ms", time_ms)
  
    if not set_grind_process_state(True,
      [
        p[-circle_diam_m / 2.0, 0, 0, 0, 0, 0],
